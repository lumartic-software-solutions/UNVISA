<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<!-- Register css -->
		<template id="assets_backend" name="Open Mobile Assets" inherit_id="web.assets_backend">
			<xpath expr=".">
				<link rel="stylesheet" href="/custom_unvisa/static/src/css/custom.css" />
			</xpath>
		</template>
		
		<template id="assets_common_report" inherit_id="report.assets_common" name="Report assets">
            <xpath expr='.' position="inside">
                <link rel="stylesheet" href="/custom_unvisa/static/src/css/report.css" />
            </xpath>
        </template>
	</data>
</odoo>
